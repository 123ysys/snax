              "sourceCode": "#include <Std>\n\nTexture2D sourceMap : TEXTURE1;\nTexture1D<float> lumTex : TEXTURE2;\n\nsampler sam : STATIC_SAMPLER1;\n \n\nstruct VO\n{\n\tfloat4 position   \t: SV_Position;\n\tfloat2 texcoord0 \t: TEXCOORD0;\n};\n\nfloat3 ACESFilm(float3 x)\n{\n    float a = 2.51f;\n    float b = 0.03f;\n    float c = 2.43f;\n    float d = 0.59f;\n    float e = 0.14f;\n    return saturate((x*(a*x+b))/(x*(c*x+d)+e));\n}\n\nfloat Tonemap_ACES(float x) {\n    // Narkowicz 2015, \"ACES Filmic Tone Mapping Curve\"\n    const float a = 2.51;\n    const float b = 0.03;\n    const float c = 2.43;\n    const float d = 0.59;\n    const float e = 0.14;\n    return (x * (a * x + b)) / (x * (c * x + d) + e);\n}\n\ncbuffer cb2 : CBUFFER1\n{\n\tfloat avgLum;\n};\n\nfloat3 rgb_to_Yxy(float3 rgb)\n{\n//    const float3x3 RGB2XYZ = { 0.5141364, 0.3238786,  0.16036376,\n //                            0.265068,  0.67023428, 0.06409157,\n  //                           0.0241188, 0.1228178,  0.84442666 };     \n\n    const float3x3 RGB2XYZ = { 0.4887180,  0.3106803,  0.2006017,\n 0.1762044,  0.8129847,  0.0108109,\n 0.0000000,  0.0102048,  0.9897952\t\t\t\t\t\t\t };\n\n\t\t\t\t\t\t\t \n\t\t\t\t\t\t\t                                \n    float3 XYZ = mul( RGB2XYZ, rgb.rgb );\n\n\t// XYZ -> Yxy conversion\n\tfloat xyzSum = XYZ.r + XYZ.g + XYZ.b;\n\tfloat3 Yxy;\n\tYxy.r = XYZ.g;                        // luminance Y\n\tYxy.g = XYZ.r / ( xyzSum );            // x = X / (X + Y + Z)\n\tYxy.b = XYZ.g / ( xyzSum );            // y = Y / (X + Y + Z)\n\treturn Yxy;\n}\n\nfloat3 Yxy_to_rgb(float3 Yxy)\n{\n\t// Yxy -> XYZ conversion\n\tfloat3 XYZ;\n\tXYZ.r = Yxy.r * Yxy.g / Yxy. b;                        // X = Y * x / y\n\tXYZ.g = Yxy.r;                                        // luminance Y\n\tXYZ.b = Yxy.r * ( 1.0 - Yxy.g - Yxy.b ) / Yxy.b;    // Z = Y * (1-x-y) / y\n\n\t// XYZ -> RGB conversion\n/*\tconst float3x3 XYZ2RGB  = { 2.5651,-1.1665,-0.3986,\n\t                          -1.0217, 1.9777, 0.0439, \n\t                           0.0753, -0.2543, 1.1892};\n\t*/\t\t\t\t\t\t   \n\t\t\tconst float3x3 XYZ2RGB  = {\t\t\t\t\t   2.3706743, -0.9000405, -0.4706338,\n-0.5138850,  1.4253036,  0.0885814,\n 0.0052982, -0.0146949,  1.0093968};\n\treturn mul(XYZ2RGB, XYZ);\n}\nfloat Reinhard2(float x) {\n    const float L_white = 2;\n    return (x * (1.0 + x / (L_white * L_white))) / (1.0 + x);\n}\n\nfloat4 main(VO IN) : SV_Target\n{\n//\tfloat luminance = avgLum;//lumTex[0];\n\n\tfloat4 color = sourceMap.SampleLevel(defaultLSampler, IN.texcoord0.xy, 0);\n\t\n\tfloat3 Yxy = rgb_to_Yxy(color.rgb);\n\t\n\tfloat lp = Yxy.x / (9.6 * avgLum + 0.0001)*2;\n\t\nYxy.x =\tReinhard2(lp);\n\n\tcolor.rgb = \tpow(Yxy_to_rgb(Yxy),1.0/1.0);\n\treturn color;\n\t/*\n\t\n\tfloat EV = log2(luminance)*(100.0/12.5);\n\t//luminance = clamp(luminance, 0.1, 5);\n\tcolor.rgb = color.rgb*EV;///(30.0*luminance);\n\n\treturn color;// float4(pow(ACESFilm(color.rgb), 1.0/1.0),1.0);\n\t*/\n\tfloat3 white_point = float3(1,1,1);\n\tfloat exposure = 0.2;\n    color.xyz = pow(abs(float3(1.0,1.0,1.0) - exp(-color.xyz / white_point * exposure)), float3((1.0 / 1.0).xxx));\n\treturn float4(color.xyz,1);\n}"
            },
