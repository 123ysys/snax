              "shaderModel": "SM51",
              "shaderType": "CS",
              "sourceCode": "\n#include <Std>\n\ncbuffer cb2 : CBUFFER1\n{\n\tfloat avgLum;\n};\n\nRWTexture2D<float4> hdrTex : UAV1;\n\nfloat3 rgb_to_Yxy(float3 rgb)\n{\n//\tconst float3x3 RGB2XYZ = { 0.5141364, 0.3238786,  0.16036376, 0.265068,  0.67023428, 0.06409157, 0.0241188, 0.1228178,  0.84442666 };\n\tconst float3x3 RGB2XYZ = { 0.4887180, 0.3106803, 0.2006017, 0.1762044, 0.8129847, 0.0108109, 0.0000000, 0.0102048, 0.9897952 }; // CIE RGB (white: E)\n\t\t\t\t\t                                \n\tfloat3 XYZ = mul( RGB2XYZ, rgb );\n\tif ((XYZ.r + XYZ.g + XYZ.b) < 1.0e-3)\n\t\treturn float3(0,0,0);\t\n\n\t// XYZ -> Yxy conversion\n\treturn float3(XYZ.g, XYZ.rg / (XYZ.r + XYZ.g + XYZ.b));\n}\n\nfloat3 Yxy_to_rgb(float3 Yxy)\n{\n\tif (Yxy.b < 1.0e-3)\n\t\treturn float3(0,0,0);\n\t\t\n\t// Yxy -> XYZ conversion\n\tfloat3 XYZ;\n\tXYZ.r = Yxy.r * Yxy.g / Yxy. b;\n\tXYZ.g = Yxy.r;\n\tXYZ.b = Yxy.r * ( 1.0 - Yxy.g - Yxy.b ) / Yxy.b;\n\n\t// XYZ -> RGB conversion\n//\tconst float3x3 XYZ2RGB  = { 2.5651,-1.1665,-0.3986, -1.0217, 1.9777, 0.0439, 0.0753, -0.2543, 1.1892};\n\tconst float3x3 XYZ2RGB  = { 2.3706743, -0.9000405, -0.4706338, -0.5138850,  1.4253036,  0.0885814, 0.0052982, -0.0146949,  1.0093968}; // CIE RGB (white: E)\n\n\treturn mul(XYZ2RGB, XYZ);\n}\n\nfloat Tonemap_ACES(float x) \n{\n\t// Narkowicz 2015, \"ACES Filmic Tone Mapping Curve\"\n\tconst float a = 2.51;\n\tconst float b = 0.03;\n\tconst float c = 2.43;\n\tconst float d = 0.59;\n\tconst float e = 0.14;\n\treturn (x * (a * x + b)) / (x * (c * x + d) + e);\n}\n\nfloat Tonemap_Reinhard2(float x, float L_white) \n{\n\treturn (x * (1.0 + x / (L_white * L_white))) / (1.0 + x);\n}\n\n\n[numthreads(16, 16, 1)]\nvoid main(uint3 DTid : SV_DispatchThreadID)\n{\n\tuint w = 0, h = 0;\n\thdrTex.GetDimensions(w,h);\n\tif (DTid.x >= w || DTid.y >= h)\n\t\treturn;\n\n\tfloat4 color = hdrTex[DTid.xy];\n\t\n\tfloat3 Yxy = rgb_to_Yxy(color.rgb);\n\t\n\tfloat lp = Yxy.x / (9.6 * avgLum + 0.0001);\n\t\n\tYxy.x =\tTonemap_ACES(lp);\n\n\tcolor.rgb = Yxy_to_rgb(Yxy);\n\tcolor.a = sqrt(dot(color.rgb, float3(0.299, 0.587, 0.114))); // alpha is luminance for FXAA\n\n\thdrTex[DTid.xy] = color;\n}\n\n"
            },
