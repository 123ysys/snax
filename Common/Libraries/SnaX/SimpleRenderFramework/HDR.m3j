              "sourceCode": "\n\n#include <Std>\n\nTexture2D<float3> hdrTex : TEXTURE1;\nTexture2D<float> luminanceTex : TEXTURE2;\n\nRWTexture2D<float4> ldrTex : UAV1;\n\n\n[numthreads(16, 16, 1)]\nvoid main(uint3 DTid : SV_DispatchThreadID)\n{\n    uint2 tsize;\n\tldrTex.GetDimensions(tsize.x, tsize.y);\n\tif (DTid.x >= tsize.x || DTid.y >= tsize.y)\n\t \treturn;\t\n\t\t \n    float lum = luminanceTex[uint2(0,0)];\n\tfloat3 white_point = float3(10,10,10);\n\tfloat exposure = 0.1;\n    float3 c =  hdrTex[DTid.xy];\n//return float4(c,1);//\n\tfloat3 white_point2 = float3(1,1,1);\n\tfloat exposure2 = 0.003;\n    c = pow(abs(float3(1.0,1.0,1.0) - exp(-c.xyz / white_point2 * exposure2)), float3((1.0 / 1.0).xxx));\n\n\t// RGBL for FXAA with L gamma encoded.\n\tfloat4 output = float4(pow(c.rgb,1.0/2.2), sqrt(dot(c.rgb, float3(0.299, 0.587, 0.114))));\n\t\n\tldrTex[DTid.xy] = output;\n\n\treturn;// output;\n\t\n\t/*\n//\tlum = pow(lum*0.00005,0.333)/0.00005;\n\nfloat ExposureBias = lum*0.000000015;\n   float3 curr = Uncharted2Tonemap(ExposureBias*c);\n\n   float3 whiteScale = 1.0f/Uncharted2Tonemap(11.2);\n   float3 color = curr*whiteScale;\n      \n   //float3 retColor = pow(color,1/2.2);\n  // return float4(color,1);\n   \n   exposure=1.0/lum*10000;\n\n    lum=max(lum,0.0001);\n\n    float key = lum*0.0001;// 1.03-(2.0/(2.0+log(lum+1.0)));// lerp(0.001,0.2, smoothstep(log(0.00005),log(5000),log(lum)));\n\tif (lum < 0.05)\n\t\tkey += (1.0-smoothstep(0.0001, 0.05, lum))*0.001;\n\t\n\t//key = 0.18*4*(2*log2(lum)-log2(0.00001)-log2(3000))/(log2(3000)-log2(0.00001));\n\t\n   \tfloat3 C = key/lum*c;\n\t  // return float4(key/lum,0,0,1);\n\treturn float4(C*(1.0+C/white_point)/(1+C), 1);\n\t*/\n// \treturn float4(pow(abs(float3(1.0,1.0,1.0) - exp(-c.xyz / white_point * exposure)), float3((1.0 / 1.0).xxx)),1);\n\t\t \n\t\t \n\t\t \n//\tfloat2 tsizeInv = 1.0 / (float2)tsize;\n//\tfloat2 halfTexelSize = float2(0.5, 0.5) * tsizeInv;\n//    float2 coord = (float2)DTid.xy * tsizeInv + halfTexelSize;\n//\tfloat3 color = source.SampleLevel(defaultLSampler, coord, 0).rgb; \n\n//\tfloat e = 0.000001;\n\t\n//\tfloat luminance = log(e + dot(color, float3(0.2126, 0.7152, 0.0722)));\n\n}\n\n"
