              "shaderModel": "SM51",
              "shaderType": "CS",
              "sourceCode": "\n#include <Std>\n\nstruct Constants \n{\n\tuint3 targetSize;\n\tuint2 textureSize;\n\tfloat2 texelSize;\n\tuint useKarisFilter;\n};\n\nConstantBuffer<Constants> constants : CBUFFER1;\n\n\nTexture2D<float4> hdrTex : TEXTURE1;\nRWTexture2D<float4> downsampledTex : UAV1;\n\nfloat luma(float4 color)\n{\n\treturn dot(color.xyz, float3(0.2126, 0.7152, 0.0722));//max(max(color.x,color.y),color.z);// ;\n}\n\nfloat4 filter(float4 a, float4 b, float4 c, float4 d)\n{\n\tfloat4 l = float4(1.0/(1.0 + luma(a)), 1.0/(1.0 + luma(b)), 1.0/(1.0 + luma(c)), 1.0/(1.0 + luma(d)));\n\tfloat4 sum = float4(0,0,0,0);\n\tsum += float4(a.xyz,1) / (1.0+luma(a));\n\tsum += float4(b.xyz,1) / (1.0+luma(b));\n\tsum += float4(c.xyz,1) / (1.0+luma(c));\n\tsum += float4(d.xyz,1) / (1.0+luma(d));\n\treturn float4(sum.xyz/sum.w,1);\n}\n\n[numthreads(16, 16, 1)]\nvoid main(uint3 DTid : SV_DispatchThreadID)\n{\n\tuint w=0, h=0,m=0;\n\tdownsampledTex.GetDimensions(w,h);\n\tif (DTid.x >= w || DTid.y >= h)\n\t\treturn;\n\tfloat2 center = (float2)DTid.xy / float2(w,h) + float2(0.5, 0.5) / float2(w,h);\n\nhdrTex.GetDimensions(w,h);\nfloat2 offset = (float2(2.0,2.0)/float2(w,h));\t\n\t\n\tfloat4 c00 = hdrTex.SampleLevel(defaultLSampler, center + float2(-offset.x, -offset.y), 0);\n\tfloat4 c10 = hdrTex.SampleLevel(defaultLSampler, center + float2(      0.0, -offset.y), 0);\n\tfloat4 c20 = hdrTex.SampleLevel(defaultLSampler, center + float2( offset.x, -offset.y), 0);\n\tfloat4 c01 = hdrTex.SampleLevel(defaultLSampler, center + float2(-offset.x,       0.0), 0);\n\tfloat4 c11 = hdrTex.SampleLevel(defaultLSampler, center + float2(      0.0,       0.0), 0);\n\tfloat4 c21 = hdrTex.SampleLevel(defaultLSampler, center + float2( offset.x,       0.0), 0);\n\tfloat4 c02 = hdrTex.SampleLevel(defaultLSampler, center + float2(-offset.x,  offset.y), 0);\n\tfloat4 c12 = hdrTex.SampleLevel(defaultLSampler, center + float2(      0.0,  offset.y), 0);\n\tfloat4 c22 = hdrTex.SampleLevel(defaultLSampler, center + float2( offset.x,  offset.y), 0);\n\tfloat4 d00 = hdrTex.SampleLevel(defaultLSampler, center + float2(-offset.x*0.5, -offset.y*0.5), 0);\n\tfloat4 d10 = hdrTex.SampleLevel(defaultLSampler, center + float2( offset.x*0.5, -offset.y*0.5), 0);\n\tfloat4 d01 = hdrTex.SampleLevel(defaultLSampler, center + float2(-offset.x*0.5,  offset.y*0.5), 0);\n\tfloat4 d11 = hdrTex.SampleLevel(defaultLSampler, center + float2( offset.x*0.5,  offset.y*0.5), 0);\n\n\tif (constants.useKarisFilter==0) {\n\t\tdownsampledTex[DTid.xy] = \n\t\t           filter(c00, c10, c01, c11)/8.0 + \n\t\t\t\t   filter(c10, c20, c11, c21)/8.0 + \n\t\t\t\t   filter(c01, c11, c02, c12)/8.0 + \n\t\t\t\t   filter(c11, c21, c12, c22)/8.0 + \n\t\t\t\t   filter(d00, d10, d01, d11)/2.0;\n\t}\t\n\telse\n\tdownsampledTex[DTid.xy] = ((c00+c10+c01+c11)/4.0/8.0+(c10+c20+c11+c21)/4.0/8.0+(c01+c11+c02+c12)/4.0/8.0+(c11+c21+c12+c22)/4.0/8.0+(d00+d10+d01+d11)/4/2);\n\n//\tdownsampledTex[DTid.xy] = r;\n}\n\n"
